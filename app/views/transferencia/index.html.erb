<p id="notice"><%= notice %></p>

<h1>Listing Transferencia</h1>

<table>
  <thead>
    <tr>
      <th>Codigo alfa numerico</th>
      <th>Tipo</th>
      <th>Valor</th>
      <th>Estorno</th>
      <th>Codigo estorno</th>
      <th>Conta debitada saldo</th>
      <th>Conta creditada saldo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @transferencia.each do |transferencium| %>
      <tr>
        <td><%= transferencium.codigo_alfa_numerico %></td>
        <td><%= transferencium.tipo %></td>
        <td><%= transferencium.valor %></td>
        <td><%= transferencium.estorno %></td>
        <td><%= transferencium.codigo_estorno %></td>
        <td><%= transferencium.conta_debitada_saldo %></td>
        <td><%= transferencium.conta_creditada_saldo %></td>
        <td><%= link_to 'Show', transferencium %></td>
        <td><%= link_to 'Edit', edit_transferencium_path(transferencium) %></td>
        <td><%= link_to 'Destroy', transferencium, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Transferencium', new_transferencium_path %>
